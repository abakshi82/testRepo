<?xml version="1.0" encoding="UTF-8"?>
<CustomObject xmlns="http://soap.sforce.com/2006/04/metadata">
    <description>Configuration of notification to preset email addresses for failed communications and exceptions.</description>
    <label>Error Notification Config</label>
    <pluralLabel>Error Notification Config</pluralLabel>
    <visibility>Public</visibility>
    <fields>
        <fullName>Application_Name__c</fullName>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Application Name</label>
        <length>255</length>
        <required>false</required>
        <type>Text</type>
        <unique>false</unique>
    </fields>
    <fields>
        <fullName>Email_Addresses__c</fullName>
        <description>Email addresses that need to receive the notification. Email address should be separated by comma.</description>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Email Addresses</label>
        <required>false</required>
        <type>TextArea</type>
    </fields>
    <fields>
        <fullName>Error_Notification_Enabled__c</fullName>
        <defaultValue>false</defaultValue>
        <externalId>false</externalId>
        <fieldManageability>DeveloperControlled</fieldManageability>
        <label>Error Notification Enabled</label>
        <type>Checkbox</type>
    </fields>
    <listViews>
        <fullName>All</fullName>
        <columns>MasterLabel</columns>
        <columns>Application_Name__c</columns>
        <columns>Email_Addresses__c</columns>
        <columns>Error_Notification_Enabled__c</columns>
        <filterScope>Everything</filterScope>
        <label>All</label>
    </listViews>
</CustomObject>
